<form action="/edit" method="POST" class="editForm hiddenDiv">
  <input type="hidden" id="hidEditInput" name="id" />
  <div class="inputContainer">
    <label for="f_name_edit">First Name:</label>
    <input type="text" name="f_name_edit" id="f_name_edit" autocomplete="off" />
  </div>
  <div class="inputContainer">
    <label for="l_name_edit">Last Name:</label>
    <input type="text" name="l_name_edit" id="l_name_edit" autocomplete="off" />
  </div>
  <div class="buttonsCont">
    <button type="button" class="delCol" id="cancelEdit">Cancel</button>
    <button type="submit">Submit Edit</button>
  </div>
</form>
